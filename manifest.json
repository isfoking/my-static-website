{
  "id": "game_pwa",
  "name": "HAPPY DOMINO",
  "short_name": "HAPPY DOMINO",
  "start_url": "./index.html",
  "dir": "auto",
  "lang": "id",
  "orientation": "any",
  "display": "fullscreen",
  "icons": [
    {
      "src": "./img/icons/logo192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "./img/icons/logo512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "description": "Best SLOT in Indonesia!",
  "categories": ["games"],
  "theme_color": "#000000",
  "background_color": "#000000",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "screenshots": [
    {
      "src": "./img/assets/a1.webp",
      "type": "image/webp",
      "sizes": "526x296"
    },
    {
      "src": "./img/assets/a2.webp",
      "type": "image/webp",
      "sizes": "526x296"
    },
    {
      "src": "./img/assets/a3.webp",
      "type": "image/webp",
      "sizes": "526x296"
    },
    {
      "src": "./img/assets/a4.webp",
      "type": "image/webp",
      "sizes": "526x296"
    }
  ],
  "display_override": ["window-controls-overlay", "standalone", "browser"],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "file_handlers": [
    {
      "action": "/open-pdf",
      "accept": {
        "application/pdf": [".pdf"]
      },
      "icons": [
        {
          "src": "pdf-icon.png",
          "sizes": "256x256",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    },
    {
      "action": "/show-model",
      "accept": {
        "application/sla": ".stl",
        "application/octet-stream": ".fbx"
      },
      "icons": [
        {
          "src": "3d-printer-icon.png",
          "sizes": "256x256",
          "type": "image/png"
        }
      ],
      "launch_type": "multiple-clients"
    }
  ],
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+music",
      "url": "/play?track=%s"
    }
  ]
}
